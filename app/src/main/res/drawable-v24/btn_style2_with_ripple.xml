<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/whitegray_project">
    <item android:id="@android:id/background">
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <corners
                android:radius="7dp"
                />
            <solid
                android:color="#211F22"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <stroke
                android:width="2dp"
                android:color="#282829"
                />
        </shape>
    </item>
</ripple>

